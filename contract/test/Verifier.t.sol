pragma solidity ^0.8.13;

import {Test, console} from "forge-std/Test.sol";
import {HonkVerifier, IVerifier} from "../src/Verifier.sol";

contract VerifierTest is Test {
    HonkVerifier public honkVerifier;

    function setUp() public {
        honkVerifier = new HonkVerifier();
    }

    function test_verifier() public {
        // function verify(bytes calldata proof, bytes32[] calldata publicInputs)
        bytes memory proof = hex"";
        bytes32[] memory publicInputs = new bytes32[](2);
        publicInputs[0] = 0x04df209ed0aad0968c3aa95d735485f04ed83fb29173287fda6716461da5815d;
        publicInputs[1] = 0x00000000000000000000000000000000000000000000000000004515514510e2;
        assertTrue(
            IVerifier(honkVerifier).verify(proof, publicInputs),
            "Proof verification failed"
        );
    }
}
